#! /usr/bin/env bash

# Applies default settings.

set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'

SCRIPT_PATH="$BETTER_CODE_CONFIG_PATH/bin/apply_default_settings"

if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: Default settings script does not exist or is not executable.\n"
fi
