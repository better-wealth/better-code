#! /usr/bin/env bash

# Performs restoration of backup data.

set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'

SCRIPT_PATH="$BETTER_CODE_CONFIG_PATH/bin/restore_backup"

if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: Restore backup script does not exist or is not executable.\n"
fi
